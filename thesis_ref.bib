@article{Lambert2006,
abstract = {In many application scenarios, the use of Regions of Interest (ROIs) within video sequences is a useful concept. It is shown in this paper how Flexible Macroblock Ordering (FMO), defined in H.264/AVC as an error resilience tool, can be used for the coding arbitrary-shaped ROIs. In order to exploit the coding of ROIs in an H.264/AVC bitstream, a description-driven content adaptation framework is introduced that is able to extract the ROIs of a given bitstream. The results of a series of tests indicate that the ROI extraction process significantly reduces the bit rate of the bitstreams and increases the decoding speed. In case of a fixed camera and a static background, the impact of this reduction on the visual quality of the video sequence is negligible. Regarding the adaptation framework itself, it is shown that in all cases, the framework operates in real time and that it is suited for streaming scenarios by design.},
author = {Lambert, Peter and {De Schrijver}, Davy and {Van Deursen}, Davy and {De Neve}, Wesley and Dhondt, Yves and {Van de Walle}, Rik},
doi = {10.1007/11864349_40},
file = {:Users/merg/Desktop/articles/2006.09 - ACIVS 2006 - Peter Lambert et al. - A Real-time Content Adaptation Framework for Exploiting ROI Scalability in H.264-AVC.pdf:pdf},
isbn = {3-540-44630-3, 978-3-540-44630-9},
issn = {16113349},
journal = {Advanced Concepts for Intelligent Vision Systems},
pages = {442--453},
title = {{A Real-Time Content Adaptation Framework for Exploiting ROI Scalability in H.264/AVC}},
url = {http://www.springerlink.com/content/x8262q66r187368g},
year = {2006}
}
