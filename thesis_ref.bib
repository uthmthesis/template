file = {:Users/merg/Desktop/articles/06882296.pdf:pdf},
issn = {15361284},
journal = {IEEE Wireless Communications},
number = {4},
pages = {54--61},
title = {{Energy-efficient multimedia transmissions through base station cooperation over heterogeneous cellular networks exploiting user behavior}},
volume = {21},
year = {2014}
}
